{
	"name": "triton-layout-viewer",
	"description": "Triton Layout Viewer - Extension for Visualizing Triton Layouts",
	"version": "0.0.2",
	"icon": "media/icon_medium.png",
	"publisher": "hmalgewatta",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/hmalgewatta/tritonlayoutviewer.git"
	},
	"engines": {
		"vscode": "^1.96.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tritonlayoutviewer.findDeclaration",
				"title": "Find #Declaration"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "tritonlayoutviewer.findDeclaration",
					"group": "navigation",
					"when": "editorHasSelection"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0"
	}
}
